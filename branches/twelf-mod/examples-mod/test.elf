%sig R = {
  a : type.
  c : a.
}.

%sig S = {
  a' : type.
  b' : a'.
  %struct r' : R = {a := a'.}.
  c' : r'.a.
  
  d' : a' -> type.
  test : d' b'.
}.

%sig T = {
  a'' : type.
  b'' = a'' -> a''.
  %struct r'' : R = {a := a'' -> a''.}.
  %struct s'': S = {a' := b''. %struct r' := r''.}. %% need to check: s''(r'a) = ra
}.

%sig U = {
%struct t''' : T = {}.
}.